<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- [QDH17]  -->
        <record model="survey.survey" id="QDH17">
            <field name="title">[QDH17] </field>
            <field name="stage_id" ref="survey.stage_in_progress"/>
            <field name="auth_required" eval="False"/>
            <field name="users_can_go_back" eval="True"/>
            <field name="description">&lt;p&gt;JCAFB 2017 - Questionário - Diabetes, Hipertensão Arterial e Hipercolesterolemia&lt;/p&gt;</field>
            <field name="thank_you_message">&lt;p&gt;Obrigado por sua participação.&lt;/p&gt;</field>
        </record>

            <!-- [QDH17_01] Identificação do Questionário -->
            <record model="survey.page" id="QDH17_01">
                <field name="title">[QDH17_01] Identificação do Questionário</field>
                <field name="survey_id" ref="QDH17"/>
                <field name="sequence" eval="10"/>
                <field name="description">&lt;p&gt;&lt;/p&gt;</field>
            </record>

                <!-- [QDH17_01_01] Código do Questionário: -->
                <record model="survey.question" id="QDH17_01_01">
                    <field name="question">[QDH17_01_01] Código do Questionário:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_01"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Questionário.</field>
                </record>

                <!-- [QDH17_01_02] Data da Entrevista (dd-mm-aaaa): -->
                <record model="survey.question" id="QDH17_01_02">
                    <field name="question">[QDH17_01_02] Data da Entrevista (dd-mm-aaaa):</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_01"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com a data da entrevista.</field>
                </record>

                <!-- [QDH17_01_03] Códigos do(s) Entrevistador(es) (separados por ","): -->
                <record model="survey.question" id="QDH17_01_03">
                    <field name="question">[QDH17_01_03] Códigos do(s) Entrevistador(es) (separados por ","):</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_01"/>
                    <field name="sequence" eval="30"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar o(s) Código(s) do(s) entrevistador(es).</field>
                </record>

            <!-- [QDH17_02] Identificação do Paciente/Entrevistado -->
            <record model="survey.page" id="QDH17_02">
                <field name="title">[QDH17_02] Identificação do Paciente/Entrevistado</field>
                <field name="survey_id" ref="QDH17"/>
                <field name="sequence" eval="20"/>
                <field name="description">&lt;p&gt;&lt;/p&gt;</field>
            </record>

                <!-- [QDH17_02_01] Nome do Paciente: -->
                <record model="survey.question" id="QDH17_02_01">
                    <field name="question">[QDH17_02_01] Nome do Paciente:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_02"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Nome do Paciente.</field>
                </record>

                <!-- [QDH17_02_02] Código do Paciente: -->
                <record model="survey.question" id="QDH17_02_02">
                    <field name="question">[QDH17_02_02] Código do Paciente:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_02"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Paciente.</field>
                </record>

                <!-- [QDH17_02_03] Nome do Endereço -->
                <record model="survey.question" id="QDH17_02_03">
                    <field name="question">[QDH17_02_03] Nome do Endereço</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_02"/>
                    <field name="sequence" eval="30"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Nome do Endereço em que o Paciente reside.</field>
                </record>

                <!-- [QDH17_02_04] Código do Endereço -->
                <record model="survey.question" id="QDH17_02_04">
                    <field name="question">[QDH17_02_04] Código do Endereço</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="QDH17_02"/>
                    <field name="sequence" eval="40"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Endereço em que o Paciente reside.</field>
                </record>

                <!-- [QDH17_02_05] Entrevistado -->
                <record model="survey.question" id="QDH17_02_05">
                    <field name="question">[QDH17_02_05] Entrevistado</field>
                    <field name="type">simple_choice</field>
                    <field name="page_id" ref="QDH17_02"/>
                    <field name="sequence" eval="50"/>
                    <field name="display_mode">columns</field>
                    <field name="column_nb">12</field>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar quem respondeu ao questionário.</field>
                    <field name="comments_allowed">True</field>
                    <field name="comments_message">Nome e grau de parentesco do Responsável ou do Cuidador; se "Outro", especificar:</field>
                </record>

                    <record model="survey.label" id="QDH17_02_05_01">
                        <field name="value">a) Responsável</field>
                        <field name="question_id" ref="QDH17_02_05"/>
                        <field name="sequence" eval="10"/>
                    </record>

                    <record model="survey.label" id="QDH17_02_05_02">
                        <field name="value">b) Paciente</field>
                        <field name="question_id" ref="QDH17_02_05"/>
                        <field name="sequence" eval="20"/>
                    </record>

                    <record model="survey.label" id="QDH17_02_05_03">
                        <field name="value">c) Cuidador</field>
                        <field name="question_id" ref="QDH17_02_05"/>
                        <field name="sequence" eval="30"/>
                    </record>

                    <record model="survey.label" id="QDH17_02_05_04">
                        <field name="value">c) Outro</field>
                        <field name="question_id" ref="QDH17_02_05"/>
                        <field name="sequence" eval="40"/>
                    </record>

    </data>
</openerp>
