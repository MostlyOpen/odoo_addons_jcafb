<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- [FSE17] JCAFB 2017 - Questionário Socioeconômico Familiar (Crianças e Idosos) -->
        <record model="survey.survey" id="FSE17">
            <field name="title">[FSE17] JCAFB 2017 - Questionário Socioeconômico Familiar (Crianças e Idosos)</field>
            <field name="stage_id" ref="survey.stage_in_progress"/>
            <field name="auth_required" eval="False"/>
            <field name="users_can_go_back" eval="True"/>
            <field name="description">&lt;p&gt;[FSE17] &lt;/p&gt;</field>
            <field name="thank_you_message">&lt;p&gt;Obrigado por sua participação.&lt;/p&gt;</field>
        </record>

            <!-- [FSE17_01] Identificação do Questionário -->
            <record model="survey.page" id="FSE17_01">
                <field name="title">[FSE17_01] Identificação do Questionário</field>
                <field name="survey_id" ref="FSE17"/>
                <field name="sequence" eval="10"/>
                <field name="description">&lt;p&gt;[FSE17_01] &lt;/p&gt;</field>
            </record>

                <!-- [FSE17_01_01] Código do Questionário: -->
                <record model="survey.question" id="FSE17_01_01">
                    <field name="question">[FSE17_01_01] Código do Questionário:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="FSE17_01"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Questionário.</field>
                </record>

                <!-- [FSE17_01_02] Data da Entrevista (dd-mm-aaaa): -->
                <record model="survey.question" id="FSE17_01_02">
                    <field name="question">[FSE17_01_02] Data da Entrevista (dd-mm-aaaa):</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="FSE17_01"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com a data da entrevista.</field>
                </record>

                <!-- [FSE17_01_03] Códigos do(s) Entrevistador(es) (um em cada linha): -->
                <record model="survey.question" id="FSE17_01_03">
                    <field name="question">[FSE17_01_03] Códigos do(s) Entrevistador(es) (um em cada linha):</field>
                    <field name="type">free_text</field>
                    <field name="page_id" ref="FSE17_01"/>
                    <field name="sequence" eval="30"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar os Códigos quem fez a entrevista.</field>
                </record>

            <!-- [FSE17_02] Identificação do Endereço/Entrevistado -->
            <record model="survey.page" id="FSE17_02">
                <field name="title">[FSE17_02] Identificação do Endereço/Entrevistado</field>
                <field name="survey_id" ref="FSE17"/>
                <field name="sequence" eval="20"/>
                <field name="description">&lt;p&gt;[FSE17_02] &lt;/p&gt;</field>
            </record>

                <!-- [FSE17_02_01] Nome do Endereço -->
                <record model="survey.question" id="FSE17_02_01">
                    <field name="question">[FSE17_02_01] Nome do Endereço</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="FSE17_02"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Nome do Endereço em que o(s) Paciente(s) reside(m).</field>
                </record>

                <!-- [FSE17_02_02] Código do Endereço -->
                <record model="survey.question" id="FSE17_02_02">
                    <field name="question">[FSE17_02_02] Código do Endereço</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="FSE17_02"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Endereço em que o(s) Paciente(s) reside(m).</field>
                </record>

                <!-- [FSE17_02_03] Entrevistado: -->
                <record model="survey.question" id="FSE17_02_03">
                    <field name="question">[FSE17_02_03] Entrevistado:</field>
                    <field name="type">simple_choice</field>
                    <field name="page_id" ref="FSE17_02"/>
                    <field name="sequence" eval="30"/>
                    <field name="display_mode">columns</field>
                    <field name="column_nb">12</field>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar quem respondeu ao questionário.</field>
                    <field name="comments_allowed">True</field>
                    <field name="comments_message">Se a resposta for "Outro", especificar o papel na residência:</field>
                </record>

                    <record model="survey.label" id="FSE17_02_03_01">
                        <field name="value">a) Responsável pela(s) Criança(s) da casa</field>
                        <field name="question_id" ref="FSE17_02_03"/>
                        <field name="sequence" eval="10"/>
                    </record>

                    <record model="survey.label" id="FSE17_02_03_02">
                        <field name="value">b) Paciente Idoso</field>
                        <field name="question_id" ref="FSE17_02_03"/>
                        <field name="sequence" eval="20"/>
                    </record>

                    <record model="survey.label" id="FSE17_02_03_03">
                        <field name="value">c) Cuidador (da(s) Criança(s) e/ou do(s) Idoso(s) da casa</field>
                        <field name="question_id" ref="FSE17_02_03"/>
                        <field name="sequence" eval="30"/>
                    </record>

                    <record model="survey.label" id="FSE17_02_03_04">
                        <field name="value">d) Outro (especificar)</field>
                        <field name="question_id" ref="FSE17_02_03"/>
                        <field name="sequence" eval="40"/>
                    </record>

    </data>
</openerp>
