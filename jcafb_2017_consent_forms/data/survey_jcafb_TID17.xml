<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- [TID17] JCAFB 2017 - TERMO DE CONSENTIMENTO LIVRE E ESCLARECIDO PARA REALIZAÇÃO DE EXAME DE URINA, COPROPARASITOLÓGICO, DETECÇÃO DE ANEMIA E QUESTIONÁRIO SOCIOECONÔMICO -->
        <record model="survey.survey" id="TID17">
            <field name="title">[TID17] JCAFB 2017 - TERMO DE CONSENTIMENTO LIVRE E ESCLARECIDO PARA REALIZAÇÃO DE EXAME DE URINA, COPROPARASITOLÓGICO, DETECÇÃO DE ANEMIA E QUESTIONÁRIO SOCIOECONÔMICO</field>
            <field name="stage_id" ref="survey.stage_in_progress"/>
            <field name="auth_required" eval="False"/>
            <field name="users_can_go_back" eval="True"/>
            <field name="description">&lt;p&gt;[TID17] &lt;/p&gt;</field>
            <field name="thank_you_message">&lt;p&gt;Obrigado por sua participação.&lt;/p&gt;</field>
        </record>

            <!-- [TID17_01] Identificação do Termo de Consentimento -->
            <record model="survey.page" id="TID17_01">
                <field name="title">[TID17_01] Identificação do Termo de Consentimento</field>
                <field name="survey_id" ref="TID17"/>
                <field name="sequence" eval="10"/>
                <field name="description">&lt;p&gt;[TID17_01] &lt;/p&gt;</field>
            </record>

                <!-- [TID17_01_01] Código do Termo de Consentimento: -->
                <record model="survey.question" id="TID17_01_01">
                    <field name="question">[TID17_01_01] Código do Termo de Consentimento:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_01"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Termo de Consentimento.</field>
                </record>

                <!-- [TID17_01_02] Data da Assinatura (dd-mm-yyyy): -->
                <record model="survey.question" id="TID17_01_02">
                    <field name="question">[TID17_01_02] Data da Assinatura (dd-mm-yyyy):</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_01"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com a data da assinatura do Termo de Consentimento.</field>
                </record>

                <!-- [TID17_01_03] Código do Professor Responsável: -->
                <record model="survey.question" id="TID17_01_03">
                    <field name="question">[TID17_01_03] Código do Professor Responsável:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_01"/>
                    <field name="sequence" eval="30"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Professor Responsável que assinou o Termo de Consentimento.</field>
                </record>

                <!-- [TID17_01_04] Códigos do(s) Coletor(es) (um em cada linha): -->
                <record model="survey.question" id="TID17_01_04">
                    <field name="question">[TID17_01_04] Códigos do(s) Coletor(es) (um em cada linha):</field>
                    <field name="type">free_text</field>
                    <field name="page_id" ref="TID17_01"/>
                    <field name="sequence" eval="40"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar os Códigos de quem colheu a assinatura do Termo.</field>
                </record>

            <!-- [TID17_02] Identificação do Indivíduo -->
            <record model="survey.page" id="TID17_02">
                <field name="title">[TID17_02] Identificação do Indivíduo</field>
                <field name="survey_id" ref="TID17"/>
                <field name="sequence" eval="20"/>
                <field name="description">&lt;p&gt;[TID17_02] &lt;/p&gt;</field>
            </record>

                <!-- [TID17_02_01] Nome do Indivíduo: -->
                <record model="survey.question" id="TID17_02_01">
                    <field name="question">[TID17_02_01] Nome do Indivíduo:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_02"/>
                    <field name="sequence" eval="10"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Nome do Indivíduo.</field>
                </record>

                <!-- [TID17_02_02] Código do Indivíduo: -->
                <record model="survey.question" id="TID17_02_02">
                    <field name="question">[TID17_02_02] Código do Indivíduo:</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_02"/>
                    <field name="sequence" eval="20"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Indivíduo.</field>
                </record>

                <!-- [TID17_02_03] Nome do Responsável Legal -->
                <record model="survey.question" id="TID17_02_03">
                    <field name="question">[TID17_02_03] Nome do Responsável Legal</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_02"/>
                    <field name="sequence" eval="30"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Nome do Responsável Legal.</field>
                </record>

                <!-- [TID17_02_04] Código do Responsável Legal -->
                <record model="survey.question" id="TID17_02_04">
                    <field name="question">[TID17_02_04] Código do Responsável Legal</field>
                    <field name="type">textbox</field>
                    <field name="page_id" ref="TID17_02"/>
                    <field name="sequence" eval="40"/>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Preencher com o Código do Responsável Legal.</field>
                </record>

                <!-- [TID17_02_05] Quem assinou o Termo de Consentimento? -->
                <record model="survey.question" id="TID17_02_05">
                    <field name="question">[TID17_02_05] Quem assinou o Termo de Consentimento?</field>
                    <field name="type">simple_choice</field>
                    <field name="page_id" ref="TID17_02"/>
                    <field name="sequence" eval="50"/>
                    <field name="display_mode">columns</field>
                    <field name="column_nb">12</field>
                    <field name="constr_mandatory">True</field>
                    <field name="constr_error_msg">Informar quem assinou o Termo de Consentimento.</field>
                    <field name="comments_allowed">False</field>
                    <field name="comments_message"></field>
                </record>

                    <record model="survey.label" id="TID17_02_05_01">
                        <field name="value">a) Indivíduo</field>
                        <field name="question_id" ref="TID17_02_05"/>
                        <field name="sequence" eval="10"/>
                    </record>

                    <record model="survey.label" id="TID17_02_05_02">
                        <field name="value">b) Responsável Legal</field>
                        <field name="question_id" ref="TID17_02_05"/>
                        <field name="sequence" eval="20"/>
                    </record>

    </data>
</openerp>
