<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="myo_lab_test_result_urina_view_form" model="ir.ui.view">
            <field name="name">myo.lab_test.result.urina.form</field>
            <field name="model">myo.lab_test.result.urina</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Lab Test Urina" version="7.0">
                    <sheet name="lab_test_result_urina" string="Survey File">
                        <div class="oe_center" name="center">
                            <!-- Put here related fields -->
                            <label for="person_code"/>
                            <h1>
                                <field name="person_code" default_focus="1"/>
                            </h1>
                        </div>
                        <!--field name="image_medium" widget='image' class="oe_left oe_avatar"/-->
                        <div class="oe_title">
                            <label for="address_code"/>
                            <h3>
                                <field name="address_code"/>
                            </h3>
                            <label for="lab_test_code"/>
                            <h3>
                                <field name="lab_test_code"/>
                            </h3>
                            <label for="lab_test_type"/>
                            <h3>
                                <field name="lab_test_type"/>
                            </h3>
                            <group name="group_title">
                                <group name="group_title_left">
                                    <field name="gender"/>
                                    <field name="age"/>
                                    <field name="person_category"/>
                                    <field name="person_status"/>
                                    <field name="address_city"/>
                                    <field name="address_category"/>
                                    <field name="address_ditrict"/>
                                </group>
                                <group name="group_title_right">
                                    <field name="EUR_02_01"/>
                                    <field name="EUR_02_02"/>
                                    <field name="EUR_02_03"/>
                                    <field name="EUR_02_04"/>
                                    <field name="EUR_02_05"/>
                                    <field name="EUR_03_01"/>
                                    <field name="EUR_03_02"/>
                                    <field name="EUR_03_03"/>
                                    <field name="EUR_03_04"/>
                                    <field name="EUR_03_05"/>
                                    <field name="EUR_03_06"/>
                                    <field name="EUR_03_07"/>
                                    <field name="EUR_03_08"/>
                                    <field name="EUR_04_01"/>
                                    <field name="EUR_04_02"/>
                                    <field name="EUR_04_03"/>
                                    <field name="EUR_04_04"/>
                                    <field name="EUR_04_05"/>
                                    <field name="EUR_04_06"/>
                                    <field name="EUR_04_07"/>
                                    <field name="EUR_04_08"/>
                                    <field name="EUR_04_09"/>
                                    <field name="EUR_04_10"/>
                                    <field name="EUR_04_11"/>
                                    <field name="EUR_04_12"/>
                                    <field name="EUR_05_01"/>
                                </group>
                            </group>
                            <group name="group_title_bottom">
                            </group>
                        </div>
                        <div class="oe_right oe_button_box" name="button_box">
                            <!-- Put here related buttons -->
                        </div>
                        <group name="group_top">
                            <group name="group_top_left">
                            </group>
                            <group name="group_top_right">
                            </group>
                        </group>
                        <group name="group_bottom">
                        </group>
                        <notebook>
                            <page string="Notes">
                                <field name="notes" placeholder="Notes..." colspan="4"/>
                            </page>
                            <page name="settings" string="Settings" groups="myo_base.group_rmanager">
                                <group>
                                    <field name="active"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="myo_lab_test_result_urina_view_tree" model="ir.ui.view">
            <field name="name">myo.lab_test.result.urina.tree</field>
            <field name="model">myo.lab_test.result.urina</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree name="suvey_qsc17s" string="Lab Test Urina">
                    <field name="person_code"/>
                    <field name="address_code"/>
                    <field name="lab_test_code"/>
                    <field name="gender"/>
                    <field name="age"/>
                    <field name="notes"/>
                </tree>
            </field>
        </record>
        
        <record id="myo_lab_test_result_urina_view_search" model="ir.ui.view">
            <field name="name">myo.lab_test.result.urina.search</field>
            <field name="model">myo.lab_test.result.urina</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search string="Survey">
                    <field name="person_code"/>
                    <field name="address_code"/>
                    <field name="lab_test_code"/>
                    <field name="gender"/>
                    <field name="age"/>
                    <field name="notes"/>
                    <separator/>
                    <group name="group_by" string="Group By...">
                        <filter string="Address" domain="[]" context="{'group_by':'address_code'}"/>
                    </group>
                </search>
            </field>
        </record>
        
        <record id="myo_lab_test_result_urina_action" model="ir.actions.act_window">
            <field name="name">Lab Test Urina</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">myo.lab_test.result.urina</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="myo_lab_test_result_urina_view_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to add a new register.
              </p>
            </field>
        </record>
        
    </data>
</openerp>
